<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<bean id="alvex-managees-workflows-rd"
		class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent"
		depends-on="">
		<property name="resourceBundles">
			<list>
				<value>alfresco.web-extension.messages.alvex-managees-workflows</value>
			</list>
		</property>
	</bean>
	<bean id="alvex-managees-workflows-cd" class="org.springframework.extensions.config.ConfigBootstrap"
		init-method="register" depends-on="">
		<property name="configService" ref="web.config" />
		<property name="configs">
			<list>
				<value>classpath:alfresco/web-extension/alvex-managees-workflows.xml</value>
			</list>
		</property>
	</bean>
	<bean id="alvex-managees-workflows" parent="alvex-extension"
		  class="com.alvexcore.share.ManageesWorkflowsExtension"
		  depends-on="alvex-managees-workflows-rd,alvex-managees-workflows-cd" />

</beans>
